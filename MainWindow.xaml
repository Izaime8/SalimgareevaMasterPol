<Window x:Class="MasterPolSalimgareeva.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MasterPolSalimgareeva"
        mc:Ignorable="d"
        Title="SalimgareevaMasterPol" Height="450" Width="800" FontFamily="Segoe UI" Icon="Images/Мастер пол.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="25"/>
        </Grid.RowDefinitions>
        <Image Source="Images/Мастер пол.png" Width="100" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="5"></Image>
        <TextBlock Text="Мастер Пол" FontSize="40" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Button Name="BackButton" Content="Назад" HorizontalAlignment="Right" Click="BackButton_Click"></Button>
        <Frame Grid.Row="1" Name="MainFrame" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered"></Frame>
    </Grid>
</Window>
